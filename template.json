  {
    "title": "Seafile",
    "type": 1,
    "categories": [
      "Cloud",
      "FileShare"
    ],
    "platform": "linux",
    "image": "seafileltd/seafile:latest",
    "ports": [
      "8313:80/tcp"
    ],
    "env": [
      {
        "name": "SEAFILE_SERVER_HOSTNAME",
        "label": "Server URL",
        "default": "seafile.domain.com"
